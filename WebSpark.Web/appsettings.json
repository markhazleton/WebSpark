{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\websites\\WebSpark\\logs\\WebSparkWeb-log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "WebSpark"
    }
  },
  "WebSpark": {
    "LogFilePath": "C:\\websites\\WebSpark\\logs\\WebSparkWeb-log-.txt",
    "DbProvider": "SQLite",
    "ConnString": "Data Source=c:\\websites\\webspark\\WebSpark.db",
    "Salt": "SECRET-CHANGE-ME!",
    "DemoMode": false,
    "CacheEnabled": false,
    "DefaultSiteId": "3"
  }
}
