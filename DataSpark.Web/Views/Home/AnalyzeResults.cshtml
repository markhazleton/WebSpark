@{
    ViewData["Title"] = "Analysis Results";
}
<div class="container mt-4">
    <h2 class="mb-4">Analysis Results</h2>
    <div class="card mb-4">
        <div class="card-body">
            <h3>Summary Statistics</h3>
            <p>@ViewBag.SummaryStats</p>
            <h3>Skewness</h3>
            <p>Skewness: @ViewBag.Skewness (@ViewBag.SkewDirection)</p>
            <h3>Histogram</h3>
            <img src="@ViewBag.Histogram" alt="Histogram" class="img-fluid mb-3" />
            <h3>Boxplot</h3>
            <img src="@ViewBag.Boxplot" alt="Boxplot" class="img-fluid" />
        </div>
    </div>
    @if (ViewBag.ErrorMessage != null)
    {
        <div class="alert alert-danger">@ViewBag.ErrorMessage</div>
    }
</div>
